<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head') %>
</head>
<body>
    <div class="container">
        <div class="row">
            <div class="col">
                <div>
                    <input type="text" name="search" id="search">
                    <ul class="list-group" id="searchResult">
                    </ul>
                </div>
                <div>
                    <% users.forEach(u=>{ %>
                        <a href="/message/<%= u.username %>"><div class="border border-primary p-1"><%= u.username %></div></a>
                    <% }) %>
                </div>
            </div>
        </div>
    </div>
    <script src="/index.js"></script>
</body>
</html>